/**
 * Arquivo service user em angular.
 * @author Samael Pereira Sim√µes
 */
app.factory('CdsProdutosService', function($resource) {
	
	return $resource('/control-expenses/rest/user/', null, {
		update: {
			method: 'PUT'
		},
		remove: {
			method: 'DELETE'
		}
	});
	
	
	return $resource('http://169.57.156.62:3003/api/produtos/', null, {
		update: {
			method: 'PUT'
		},
		remove: {
			method: 'DELETE'
		}
	});
});