app.controller('HomeController', ['$scope', '$http', '$log','$rootScope', '$location', 'toastr', '$cookies', '$timeout','uiGridConstants',
	function($scope, $rootScope, $location, $http, $log, toastr, $cookies,  timeout, uiGridConstants) {
	
	$scope.cookie = $cookies.getObject('return');
	
	$scope.operacional;
	$scope.adminstrativo;
	$scope.userpdf = ' ';
	
	if( $scope.cookie.Status == "Operacional" ) {	
		$scope.userpdf = $scope.cookie.user
		$scope.operacional = true;
	}else{
		$scope.operacional = false
	}
	
}])
